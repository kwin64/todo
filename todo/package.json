    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "typescript": "^4.2.3",
